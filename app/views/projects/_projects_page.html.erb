<h2><%= current_user.first_name %>'s Projects</h2>
<table class="table table-hover">
  <thead>
    <th>Title</th>
  </thead>
  <tbody>
    <% current_user.active_projects.each do |project| %>
      <tr>
        <td><%= link_to "#{project.title}", project_path(project), :class => "project-list" %></td>
      </tr>
    <% end %>
  </tbody>
</table>
