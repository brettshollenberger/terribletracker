<table class="table table-hover">
  <thead>
    <th>Title</th>
    <th>User Story</th>
    <th>Estimate</th>
    <th>Complexity</th>
  </thead>
  <tbody>
    <% @user_stories.each do |story| %>
      <tr>
        <td><%= story.title %></td>
        <td><%= story.story %></td>
        <td><%= story.estimate_in_quarter_days %></td>
        <td><%= story.complexity %></td>
      </tr>
    <% end %>
  </tbody>
</table>
